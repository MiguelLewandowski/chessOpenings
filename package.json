{
  "name": "chess-openings",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "cross-env PRISMA_CLIENT_ENGINE_TYPE=binary prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "centauro:test": "tsx src/scripts/test-engine.ts",
    "centauro:run:theory": "tsx src/scripts/run-centauro.ts --mode=theory --save",
    "centauro:run:theory:dry": "tsx src/scripts/run-centauro.ts --mode=theory",
    "centauro:run:punicao": "tsx src/scripts/run-centauro.ts --mode=punicao --save",
    "centauro:run:punicao:dry": "tsx src/scripts/run-centauro.ts --mode=punicao"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "chess.js": "^1.2.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-chessboard": "^4.7.3",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.5",
    "@prisma/client": "^5.19.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "prisma": "^5.19.0",
    "tsx": "^4.19.2",
    "cross-env": "^7.0.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
